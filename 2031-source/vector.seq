 .PAGE 'VECTOR'ECHKSM .BYTE $FA ; $E - $F CHECKSUMNMI JMP (VNMI);PATCH ;$E - $F PATCH AREA;---------------------------------------------; IN THE 2031 901484-05 ROM THE PATCH AREA; IS EMPTY;---------------------------------------------;;---------------------------------------------; TABLE: SYSTEM VECTORS;----------------------------------------------------;;DEFAULT TABLE FOR USER COMMAND; *= $FFE6;-------------------------------------------------- .WORD FORMT ;FORMAT A DISK (1541 USES FORMAT) .WORD TRNOFF ;SWITCH OFF DRIVE MOTORUBLOCK .WORD UBLKRD ;U1 COMMAND .WORD UBLKWT ;U2 COMMAND;; DEFAULT TABLE FOR USER COMMAND .WORD $0500 ;LINKS TO BUFFER #2 .WORD $0503 .WORD $0506 .WORD $0509 .WORD $050C .WORD $050F; STANDARD 6502 VECTORS *= $FFFA .WORD NMI ;NMI ADDRESS .WORD DSKINT ;RESET ADDRESS .WORD SYSIRQ ;IRQ ADDRESS;.END